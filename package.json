{
  "name": "electronVueJS-App",
  "version": "0.0.4",
  "description": "A basic Electron, Vue, Vuetify, NodeJS template App that can be easily re-used and forked to make many different kinds of apps written with ES6 syntax.",
  "main": "background.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SethEden/electronVueJS-App.git"
  },
  "homepage": "https://github.com/SethEden/electronVueJS-App#readme",
  "author": "Seth Hollingsead",
  "license": "MIT",
  "private": true,
  "scripts": {
    "electron-build": "vue-cli-service electron:build",
    "electron-serve": "vue-cli-service electron:serve",
    "build": "npm run electron-build",
    "start": "npm run electron-serve",
    "vers": "standard-version -m \"chore(release) version %s [ci skip\"]",
    "docs": "jsdoc -c jsdoc.json",
    "deploy": "npm run docs && npm run build",
    "release": "npm run vers && npm run deploy"
  },
  "dependencies": {
    "chai-as-promised": "^7.1.1",
    "core-js": "^3.6.5",
    "path": "^0.12.7",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-nightwatch": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-pwa": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "chromedriver": "83",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "geckodriver": "^1.19.1",
    "jsdoc": "^3.6.4",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "spectron": "^11.0.0",
    "standard-version": "^8.0.2",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.4",
    "vue-cli-plugin-vuetify": "~2.0.6",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  }
}
